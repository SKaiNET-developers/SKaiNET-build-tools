{
  "input_file": "/input/model.mlir",
  "output_file": "/output/model.vmfb",
  "target": "metal",
  "optimization_level": "O3",
  "target_features": ["metal2.4"],
  "output_format": "vmfb",
  "validation": true,
  "benchmark": false,
  "verbose": false,
  "compilation_options": {
    "enable_assertions": false,
    "strip_debug_info": true,
    "enable_profiling": false,
    "memory_planning": "default"
  },
  "target_specific": {
    "metal": {
      "metal_version": "2.4",
      "macos_deployment_target": "11.0"
    }
  },
  "metadata": {
    "description": "Example Metal IREE compilation configuration",
    "version": "1.0",
    "author": "SKaiNET Build Tools",
    "tags": ["metal", "apple", "gpu", "macos"]
  }
}