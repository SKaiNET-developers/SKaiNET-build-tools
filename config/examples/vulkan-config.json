{
  "input_file": "/input/model.mlir",
  "output_file": "/output/model.vmfb",
  "target": "vulkan",
  "optimization_level": "O3",
  "target_features": ["spirv1.3"],
  "output_format": "vmfb",
  "validation": true,
  "benchmark": false,
  "verbose": false,
  "compilation_options": {
    "enable_assertions": false,
    "strip_debug_info": true,
    "enable_profiling": false,
    "memory_planning": "default"
  },
  "target_specific": {
    "vulkan": {
      "spirv_version": "1.3",
      "vulkan_version": "1.1"
    }
  },
  "metadata": {
    "description": "Example Vulkan IREE compilation configuration",
    "version": "1.0",
    "author": "SKaiNET Build Tools",
    "tags": ["vulkan", "gpu", "cross-platform"]
  }
}